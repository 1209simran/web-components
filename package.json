  "license": "Apache-2.0",
  "devDependencies": {
    "colors": "1.3.3",
    "lerna": "3.15.0",
    "moment-mini": "2.22.1",
    "semver": "6.2.0"
  },
  "dependencies": {
    "react-dom": "^16.13.1",
    "slate": "^0.57.2",
    "slate-history": "^0.57.2",
